{
  "extends": [
    "airbnb-base",
    "angular"
  ],
  "plugins": [
    "promise"
  ],
  "env": {
    "browser" : true
  },
  "globals": {
  },
  "rules": {
    "key-spacing"          : 0,
    "jsx-quotes"           : [2, "prefer-single"],
    "max-len"              : [2, 120, 2],
    "no-param-reassign"    : ["error", { "props": false }],
    "object-curly-spacing" : [2, "always"],
    "prefer-const"         : 0
  }
}
